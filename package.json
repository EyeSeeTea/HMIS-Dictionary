{
    "name": "hmis-dictionary",
    "description": "DHIS2 HMIS Dictionary app",
    "version": "1.3.2",
    "private": true,
    "repository": {
        "type": "git",
        "url": "git+https://github.com/EyeSeeTea/HMIS-Dictionary.git"
    },
    "scripts": {
        "prebuild": "rm -rf HMIS_Dictionary.zip",
        "build": "zip -r HMIS_Dictionary.zip .",
        "post": "npm run build; curl -X POST -u admin:district -F file=@HMIS_Dictionary.zip 'http://localhost:8080/api/apps'",
        "start": "http-server . -a ${HOST:-localhost} -p ${PORT:-8082} -c-1 -P ${DHIS:-http://localhost:8080} --proxy-options.auth $AUTH",
        "dev-start": "http-server . -a localhost -p 8082 -c-1 -P http://localhost:8080/ --proxy-options.auth admin:district"
    },
    "homepage": "https://github.com/EyeSeeTea/HMIS-Dictionary",
    "dependencies": {
        "npm": "^6.4.1",
        "lodash": "^4.17.14",
        "jquery": "^v2.2.4"
    },
    "devDependencies": {
        "http-server":  "^14.1.1"
    }
}
